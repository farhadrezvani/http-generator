{
  "name": "http-generator",
  "version": "0.0.8",
  "description": "Generate .http files from OpenAPI specifications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "swagger",
    "openapi",
    "open-api",
    "spec",
    "specification",
    "http",
    "gen",
    "generator"
  ],
  "author": {
    "name": "Farhad Rezvani",
    "url": "https://farhadrezvani.ir"
  },
  "license": "MIT",
  "bin": {
    "http-generator": "./dist/cli.js"
  },
  "repository": {
    "url": "https://github.com/farhadrezvani/http-generator"
  },
  "scripts": {
    "dev": "tsup src/cli.ts --watch",
    "build": "tsup src/cli.ts src/index.ts --clean --splitting",
    "release": "pnpm build && changeset publish",
    "lint": "tsc"
  },
  "dependencies": {
    "@readme/openapi-parser": "^2.6.0",
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.5",
    "@types/node": "^20.14.2",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5"
  }
}
